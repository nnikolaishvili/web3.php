{
    "name": "nnikolaishvili/web3.php",
    "description": "Ethereum web3 interface. Package that supports php 8",
    "type": "library",
    "license": "MIT",
    
    "require": {
        "php": "^7.1|^8.0",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0",
        "kornrunner/keccak": "~1.0",
        "phpseclib/phpseclib": "~2.0.36"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Web3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    }
}
